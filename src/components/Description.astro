---
type Props = {
  data: {
    name: string;
    icon: string;
    link: string;
    status: string;
    description: string;
  };
};

const { data } = Astro.props;
---

{
  data && data.status === "enabled" && (
    <div>
      <p class="text-center mr-16 ml-16">{data.description}</p>
    </div>
  )
}
